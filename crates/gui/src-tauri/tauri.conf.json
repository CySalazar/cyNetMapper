{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist",
    "withGlobalTauri": false
  },
  "package": {
    "productName": "cynetmapper-gui-tauri",
    "version": "0.1.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false
      },
      "dialog": {
        "all": false
      },
      "fs": {
        "all": false
      },
      "path": {
        "all": false
      },
      "protocol": {
        "all": false,
        "asset": true,
        "assetScope": [
          "**"
        ]
      },
      "os": {
        "all": false
      },
      "notification": {
        "all": false
      },
      "globalShortcut": {
        "all": false
      },
      "clipboard": {
        "all": false
      },
      "window": {
        "all": false,
        "close": true,
        "hide": true,
        "show": true,
        "maximize": true,
        "minimize": true,
        "unmaximize": true,
        "unminimize": true,
        "startDragging": true,
        "print": true,
        "setResizable": true,
        "setTitle": true,
        "setSize": true,
        "setMinSize": true,
        "setMaxSize": true,
        "setPosition": true,
        "setFullscreen": true,
        "setFocus": true,
        "setIcon": true,
        "setSkipTaskbar": true,
        "setCursorGrab": true,
        "setCursorVisible": true,
        "setCursorIcon": true,
        "setCursorPosition": true,
        "setIgnoreCursorEvents": true,
        "create": true,
        "center": false,
        "requestUserAttention": true
      }
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.cynetmapper.gui",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "resources": [],
      "externalBin": [],
      "copyright": "Copyright Â© 2024 cyNetMapper Team",
      "category": "DeveloperTool",
      "shortDescription": "Network scanner and analysis tool",
      "longDescription": "cyNetMapper is a comprehensive network scanning and analysis tool with a modern desktop interface. It provides host discovery, port scanning, service detection, OS fingerprinting, and network topology visualization.",
      "deb": {
        "depends": []
      },
      "macOS": {
        "frameworks": [],
        "minimumSystemVersion": "10.13",
        "exceptionDomain": "",
        "signingIdentity": null,
        "providerShortName": null,
        "entitlements": null
      },
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": "default-src 'self'; img-src 'self' asset: https://asset.localhost data:; style-src 'self' 'unsafe-inline'; font-src 'self' data:; script-src 'self' 'unsafe-eval'"
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "cyNetMapper",
        "width": 1200,
        "height": 800,
        "minWidth": 800,
        "minHeight": 600,
        "center": false,
        "decorations": true,
        "alwaysOnTop": false,
        "skipTaskbar": false,
        "theme": "Dark",
        "titleBarStyle": "Visible",
        "hiddenTitle": false,
        "acceptFirstMouse": false,
        "tabbingIdentifier": "main",
        "maximizable": true,
        "minimizable": true,
        "closable": true,
        "transparent": false,

        "focus": true,
        "visible": true,
        "fileDropEnabled": true,
        "url": "index.html",
        "label": "main"
      }
    ]
  }
}